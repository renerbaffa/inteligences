{
  "extends": "react-app",
  "plugins": ["prettier"],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "semi": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "all",
        "printWidth": 100
      }
    ]
  }
}
